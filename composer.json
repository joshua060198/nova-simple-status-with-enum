  "description": "Simple status field for Laravel Nova",
  "keywords": [
    "status",
    "badge",
    "enum"
  ],
  "version": "0.0.3",
  "authors": [
    {
      "name": "Joshua Lauwrich Nandy",
      "email": "joshua.lauwrich.98@gmail.com",
